{
  "name": "lambal",
  "version": "0.0.1",
  "description": "An elegant utility library to spice up your functional style code.",
  "main": "lib/index.js",
  "types": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "test": "ava src/**/*.test.ts",
    "test:watch": "ava -w src/**/*.test.ts",
    "prepare": "npm run build",
    "postversion": "git push && gist push --tags"
  },
  "author": "Michiel Kamphuis <mfk@kamphuis.eu>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^1.3.1",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
